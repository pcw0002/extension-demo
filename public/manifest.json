{
  "short_name": "React App",
  "name": "Extension Demo",
  "manifest_version": 2,
  "version": "1.0",
  "browser_action": {
    "default_popup": "index.html"
  },
  "start_url": ".",
  "display": "standalone",
  "theme_color": "#000000",
  "background_color": "#ffffff",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": ["activeTab","tabs", "*://*/*"]
}
